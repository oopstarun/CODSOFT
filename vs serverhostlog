SOURCE_DIR="/var/lib/docker/codespacemount/.persistedshare"
DEST_DIR="/destination/path"

for file in "vsserverhostlog.txt" "vsserverterminallog.txt"; do
    if [ -f "${SOURCE_DIR}/${file}" ]; then
        cp "${SOURCE_DIR}/${file}" "${DEST_DIR}/"
    else
        echo "${file} does not exist."
    fi
done
