if [ -f /var/lib/docker/codespacemount/.persistedshare/vsserverhostlog.txt ]; then
    cp /var/lib/docker/codespacemount/.persistedshare/vsserverhostlog.txt /destination/path/
else
    echo "vsserverhostlog.txt does not exist."
fi

if [ -f /var/lib/docker/codespacemount/.persistedshare/vsserverterminallog.txt ]; then
    cp /var/lib/docker/codespacemount/.persistedshare/vsserverterminallog.txt /destination/path/
else
    echo "vsserverterminallog.txt does not exist."
fi
